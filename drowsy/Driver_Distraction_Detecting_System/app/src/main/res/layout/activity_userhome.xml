    <LinearLayout
        android:layout_width="333dp"
        android:layout_height="357dp"
        android:background="@drawable/userregbg"
        android:orientation="vertical"
        android:layout_marginTop="70dp"
        android:padding="6dp">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="20dp">

            <LinearLayout
                android:layout_width="89dp"
                android:layout_height="110dp"
                android:layout_marginRight="10dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/viewmess"
                    android:layout_width="50dp"
                    android:layout_height="40dp"
                    android:layout_gravity="center"
                    android:layout_margin="5dp"
                    android:src="@drawable/warringmsgnew"
                    app:srcCompat="@drawable/warringmsgnew" />

                <TextView
                    android:id="@+id/textVsdiew8"
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="VIEW WARNING MESSAGE"
                    android:textAlignment="center"
                    android:textColor="@color/black"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="89dp"
                android:layout_height="110dp"
                android:layout_marginRight="10dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/enablebtn"
                    android:layout_width="45dp"
                    android:layout_height="38dp"
                    android:layout_gravity="center"
                    android:layout_margin="5dp"
                    android:src="@drawable/autopilot"
                    app:srcCompat="@drawable/autopilot" />

                <TextView
                    android:id="@+id/texsdtView8"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="ENABLE DRIVING MODE"
                    android:textAlignment="center"
                    android:textColor="@color/black"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="89dp"
                android:layout_height="110dp"
                android:layout_marginRight="10dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/addbtn"
                    android:layout_width="36dp"
                    android:layout_height="45dp"
                    android:layout_gravity="center"
                    android:layout_margin="5dp"
                    android:src="@drawable/statusone"
                    app:srcCompat="@drawable/statusone" />

                <TextView
                    android:id="@+id/textVdsfsiew8"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="VIEW STATUS"
                    android:textAlignment="center"
                    android:textColor="@color/black"
                    android:textStyle="bold" />
            </LinearLayout>
        </TableRow>


        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="20dp">
        </TableRow>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="110dp"
            android:layout_marginLeft="130dp"
            android:layout_marginRight="10dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/logout"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_gravity="center"
                android:layout_margin="5dp"
                android:src="@drawable/logout" />


            <TextView
                android:id="@+id/textVithrew14"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
